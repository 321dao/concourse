hostname: <%= "#{name}-#{index}" %>

destination:
  transport: <%= p("blackbox.destination.transport") %>
  address: <%= p("blackbox.destination.address") %>

sources:
<% p("blackbox.sources").each do |tag, paths| %>
<% paths.each do |path| %>
  - path: <%= path %>
    tag: <%= tag %>
<% end %>
<% end %>
